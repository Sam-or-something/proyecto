// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = ACA SOFI
  relationMode = "prisma"
}

model user {
  email String @unique @id
  name String
  lastName String
  password String
}